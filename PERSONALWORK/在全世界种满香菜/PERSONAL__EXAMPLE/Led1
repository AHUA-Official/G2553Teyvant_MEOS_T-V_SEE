#include <msp430.h>				


/**
 * blink.c
 */
void main(void)
{
	WDTCTL = WDTPW | WDTHOLD;		// stop watchdog timer
	P1DIR |= BIT6;			// configure P1.0 as output
	P1DIR &= ~BIT6;
	volatile unsigned int i;		// volatile to prevent optimization

	while(1)
	{
		P1OUT ^= BIT6;				// toggle P1.0      ^异或运算
		__delay_cycles(50000);    // delay
	}
}
